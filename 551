  <script>
    function showStemFields() {
      document.getElementById('stem-fields').style.display = 'flex';
      document.querySelectorAll('.subfields').forEach(el => el.style.display = 'none');
      document.getElementById('humanities-fields').style.display = 'none';
      document.getElementById('medicine-fields').style.display = 'none';
      document.getElementById('trades-fields').style.display = 'none';
    }
    function showHumanitiesFields() {
      document.getElementById('humanities-fields').style.display = 'flex';
      document.querySelectorAll('.subfields').forEach(el => el.style.display = 'none');
      document.getElementById('stem-fields').style.display = 'none';
      document.getElementById('medicine-fields').style.display = 'none';
      document.getElementById('trades-fields').style.display = 'none';
    }
    function showMedicineFields() {
      document.getElementById('medicine-fields').style.display = 'flex';
      document.querySelectorAll('.subfields').forEach(el => el.style.display = 'none');
      document.getElementById('stem-fields').style.display = 'none';
      document.getElementById('humanities-fields').style.display = 'none';
      document.getElementById('trades-fields').style.display = 'none';
    }
    function showTradesFields() {
      document.getElementById('trades-fields').style.display = 'flex';
      document.querySelectorAll('.subfields').forEach(el => el.style.display = 'none');
      document.getElementById('stem-fields').style.display = 'none';
      document.getElementById('humanities-fields').style.display = 'none';
      document.getElementById('medicine-fields').style.display = 'none';
    }
    function toggleSubfields(id) {
      document.querySelectorAll('.subfields').forEach(el => el.style.display = 'none');
      const subfieldEl = document.getElementById(id);
      subfieldEl.style.display = 'flex';
    }
  </script>
</head>
<body>
  <header>
    <nav>
      <a onclick="showStemFields()">STEM</a>
      <a onclick="showHumanitiesFields()">Humanities</a>
      <a onclick="showMedicineFields()">Medicine</a>
      <a onclick="showTradesFields()">Trades</a>
    </nav>
  </header>

